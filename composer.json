{
    "name": "improvit/flex-env",
    "description": "Edit your .env file in Laravel directly from the command line.",
    "keywords": ["flexible", "env", "laravel", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sven\\FlexEnv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\FlexEnv\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\FlexEnv\\FlexEnvServiceProvider"
            ]
        }
    }
}
